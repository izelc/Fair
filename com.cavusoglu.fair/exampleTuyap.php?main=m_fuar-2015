<script language="javascript">
function kontrol(str){
	var eposta = str.eposta.value;
		
	if (eposta =="" || eposta=="E-Posta"){
		alert ("Lütfen geçerli bir eposta adresi belirtiniz.");
	return false;
	}
	
	return true;

	
}
</script> 
